{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-sptfyprg-uksouth"
		},
		"adls_sptfyprg_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_sptfyprg_ls'"
		},
		"sqldb_sptfyprg_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sqldb_sptfyprg_ls'"
		},
		"adls_sptfyprg_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://strgsptfyprguksouth.dfs.core.windows.net/"
		},
		"sqldb_sptfyprg_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlsrvr-sptfyprg-uksouth.database.windows.net"
		},
		"sqldb_sptfyprg_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb-sptfyprg-uksouth"
		},
		"sqldb_sptfyprg_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminnithin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-sptfyprg-uksouth_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_sptfyprg_ls_accountKey": {
						"value": "[parameters('adls_sptfyprg_ls_accountKey')]"
					},
					"sqldb_sptfyprg_ls_password": {
						"value": "[parameters('sqldb_sptfyprg_ls_password')]"
					},
					"adls_sptfyprg_ls_properties_typeProperties_url": {
						"value": "[parameters('adls_sptfyprg_ls_properties_typeProperties_url')]"
					},
					"sqldb_sptfyprg_ls_properties_typeProperties_server": {
						"value": "[parameters('sqldb_sptfyprg_ls_properties_typeProperties_server')]"
					},
					"sqldb_sptfyprg_ls_properties_typeProperties_database": {
						"value": "[parameters('sqldb_sptfyprg_ls_properties_typeProperties_database')]"
					},
					"sqldb_sptfyprg_ls_properties_typeProperties_userName": {
						"value": "[parameters('sqldb_sptfyprg_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}